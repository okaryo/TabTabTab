{
  "name": "TabTabTab",
  "description": "This chrome extension makes it easier to manage your open tabs.",
  "private": true,
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "test": "vitest run",
    "lint": "eslint './src/**/*.{js,ts,jsx,tsx}' './test/**/*.{js,ts,jsx,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,jsx,tsx}' './test/**/*.{js,ts,jsx,tsx}'",
    "format": "prettier './src/**/*.{js,ts,jsx,tsx}' './test/**/*.{js,ts,jsx,tsx}' --check",
    "format:fix": "prettier './src/**/*.{js,ts,jsx,tsx}' './test/**/*.{js,ts,jsx,tsx}' --write",
    "locale-lint": "chromex-locale-lint --localesDir dist/_locales"
  },
  "author": "okaryo",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.244",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.0.4",
    "chromex-locale-lint": "^0.0.3",
    "eslint": "^8.48.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.7",
    "@mui/material": "^5.14.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
