{
  "manifest_version": 3,
  "name": "__MSG_app_name__",
  "short_name": "__MSG_app_name_short__",
  "description": "__MSG_app_desc__",
  "default_locale": "en",
  "version": "2.38.0",
  "action": {
    "default_title": "TabTabTab",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    }
  },
  "permissions": [
    "alarms",
    "activeTab",
    "contextMenus",
    "tabGroups",
    "tabs",
    "storage",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "side_panel.html"
  },
  "background" : {
    "service_worker" : "js/background.js",
    "type": "module"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": "Alt+Shift+T",
      "description": "Open TabTabTab popup"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["js/assets/*"],
      "matches": ["*://*/*"]
    }
  ]
}
